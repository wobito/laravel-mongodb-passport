{
    "name": "wobito/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/wobito/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [ "laravel", "laravel-mongodb", "laravel-mongodb-passport", "laravel-passport", "mongodb", "passport" ],
    "require": {
        "illuminate/support": "^5.5 || ^6.0",
        "laravel/passport": "4.0.* || 5.0.* || 6.0.* || 7.0.* || 7.4.* || 7.5.* || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wobito\\Mongodb\\": "src"
        }
    },
    "authors": [],
    "extra": {
        "laravel": {
            "providers": [ "Wobito\\Mongodb\\MongodbPassportServiceProvider" ]
        }
    }
}
